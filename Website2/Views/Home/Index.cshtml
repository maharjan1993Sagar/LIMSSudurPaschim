    </div>

</div>


<style type="text/css">
    .pos-abs .member-img {
        width: 85px;
        border: 2px solid #fff;
        border-radius: 10px;
        overflow: hidden;
    }

    .pos-rel {
        position: relative;
    }

    .pos-abs {
        height: 500px;
        position: relative;
        background: rgb(249, 248, 248);
        right: 0;
        display: flex;
        flex-direction: column;
        justify-content: center;
    }

    {
        font-weight: 500
    }

    .ticker-container ul div {
        left: 10%;
    }

        .ticker-container ul div li {
            font-size: 12px;
        }

    .pos-abs .member-name {
        color: #222;
        font-size: 13px;
    }

    .ticker-container {
        margin-top: 7px;
        margin-bottom: 7px;
        background: #fff;
        box-shadow: 0px 0px 8px #eee;
    }

    .pos-abs .member-postion {
        font-size: 0.85rem;
        color: #3F51B5;
        font-weight: 600;
        margin-top: 0px;
    }

    .pos-abs .member-desc {
        display: flex;
        flex-direction: column;
        justify-content: center;
        text-align: center;
    }

    .pos-abs .member-area:nth-last-child(1) {
        margin-bottom: 0
    }

    .pos-abs .member-area {
        display: block;
        background: transparent; /* fallback for old browsers */
        margin-bottom: 10px;
        padding: 0px;
        border-radius: 10px;
    }

    .hilight-img {
        height: 50px;
        width: 60px;
        object-fit: cover;
        float: left;
        margin-right: 15px;
        border-radius: 5px;
    }

    .highight-list {
        padding: 10px 0 10px 30px;
        float: right;
        background: white;
        width: 80%;
    }

    .highlight-label {
        background: #2B39BC;
        width: 10%;
        float: left;
        padding: 5px 0;
        padding-right: 0px;
        text-align: right;
        font-size: 14px;
    }

    .highlight-header {
        background: #2b39bc;
        color: #fff;
        padding: 10px 20px;
        margin: auto;
    }

    .highlight-section {
        background: #f9f8f8;
        border: 1px solid #eee;
        border-radius: 5px;
    }

        .highlight-section ul {
            max-height: 455px;
            overflow: auto;
            padding-left: 0;
            padding: 20px 15px;
            margin-bottom: 0;
        }

        .highlight-section li {
            font-size: 13px;
            font-weight: 500;
            overflow: hidden;
        }

        .highlight-section ul li a i {
            margin-right: 5px;
            margin-top: 5px;
        }

    .highlight-header h1 {
        margin-bottom: 0;
        font-size: 20px;
    }

    .highlight-section span.date {
        font-size: 12px;
        font-weight: normal;
        margin-left: 0px !important;
        color: #888;
    }

    @@media (max-width: 768px) {
        .pos-abs {
            position: relative;
            max-width: 100%;
            flex: 100%;
            background: transparent;
            height: auto;
        }
    }
</style>
<div class="clearfix"></div>

<script type="text/javascript">
    function PopUp(hideOrshow) {
        if (hideOrshow == 'hide') document.getElementById('ac-wrapper').style.display = "none";
        else document.getElementById('ac-wrapper').removeAttribute('style');
    }
    window.onload = function () {

        setTimeout(function () {
            PopUp('show');
        }, 300);
    }
</script>
