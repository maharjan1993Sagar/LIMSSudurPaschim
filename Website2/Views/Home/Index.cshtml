@model HomeModel
<style>
    img {
        max-width: 100% !important;
    }
</style>
<div class="row">
    <div class="trending">
        <div class="container">
            <div class="trending-now">
                <p>Trending:</p>
            </div>

            <marquee behavior="scroll" direction="left" onmouseover="this.stop();"
                     onmouseout="this.start();">
                @if (Model.NewsScroll.Any())
                {
                    foreach (var item in Model.NewsScroll)
                    {
                        <span class="news-text news-text-mar-left">
                            <a asp-controller="NewsEvent" asp-action="Index" asp-route-type="@item.TypeName" asp-route-id="@item.Id">@(item.Title + "(" + item.TypeName + ")")</a>
                        </span>
                    }
                }
                else
                {
                    <span class="news-text news-text-mar-left">
                        <a asp-controller="NewsEvent" asp-action="Index">No record found.</a>
                    </span>
                }
            </marquee>

            <div class="clearfix"></div>
        </div>
    </div>
    <br />
</div>
<div class="row pos-rel">
    <div class="col-lg-9 col-md-9 col-sm-8 col-xs-12 p-0">
        <div class="slider-area">
            <div id="demo" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">

                    @if (Model.Banner.Any())
                    {
                        @for (int i = 0; i < Model.Banner.Count; i++)
                        {
                            <div class='@(i == 0 ? "carousel-item active" : "carousel-item")'>
                                <a href="#">
                                    <img src='@Model.Banner[i].Image.PictureUrl' alt="" style="width: 100%; height: 500px; overflow: hidden;">
                                </a>
                                <div class="carousel-caption">
                                    <h3></h3>
                                    <a href="#">
                                        <p style="color:white;"> @Model.Banner[i].Title</p>
                                    </a>
                                </div>

                            </div>
                        }
                    }
                    else
                    {
                        <div class='carousel-item active'>
                            <a href="#">
                                <img src='/Dolfd/img/11.jpg' alt="" style="width: 100%; height: 500px; overflow: hidden;">
                            </a>
                            <div class="carousel-caption">
                                <h3></h3>
                                <a href="#">
                                    <p style="color:white;"> Banner title</p>
                                </a>
                            </div>
                        </div>
                        <div class='carousel-item'>
                            <a href="#">
                                <img src='/Dolfd/img/11.jpg' alt="" style="width: 100%; height: 500px; overflow: hidden;">
                            </a>
                            <div class="carousel-caption">
                                <h3></h3>
                                <a href="#">
                                    <p style="color:white;"> Banner title</p>
                                </a>
                            </div>
                        </div>
                    }

                 

                </div>
                <a class="carousel-control-prev" href="#demo" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#demo" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>
    <div class="col-2 col-lg-3 col-md-3 col-sm-4 col-xs-6 pos-abs">

        <div class="member-area">
            <div class="member-img">
                <img src='@((Model.Minister != null && (!String.IsNullOrEmpty(Model.Minister.Image.PictureUrl))) ? Model.Minister.Image.PictureUrl : "/Dolfd/img/111.png")' height="120px" width="100px" />
            </div>
            <div class="member-desc">
                <div class="member-name">
                    @(Model.Minister != null ? Model.Minister.Name : "Hon. Ministry name")                    <!-- <p style="font-weight:  normal;">सम्माननीय प्रधानमन्त्री एवं मन्त्री सञ्चार तथा सूचना प्रविधि ‍</p> -->
                </div>
                <div class="member-postion">Honorable Ministry‍</div>
            </div>
        </div>
        <div class="member-area">
            <div class="member-img">
                <img src='@((Model.StateMinister != null && (!String.IsNullOrEmpty(Model.StateMinister.Image.PictureUrl))) ? Model.StateMinister.Image.PictureUrl : "/Dolfd/img/111.png")' height="120px" width="100px" />
            </div>
            <div class="member-desc">
                <div class="member-name">
                    @(Model.StateMinister != null ? Model.StateMinister.Name : "Hon. State Ministry name")                    <!-- <p style="font-weight:  normal;">सम्माननीय प्रधानमन्त्री एवं मन्त्री सञ्चार तथा सूचना प्रविधि ‍</p> -->
                </div>
                <!-- <p style="font-weight:  normal;">सम्माननीय प्रधानमन्त्री एवं मन्त्री सञ्चार तथा सूचना प्रविधि ‍</p> -->
                <div class="member-postion">Honorable State Ministry‍</div>
            </div>
        </div>
        <div class="member-area">
            <div class="member-img">
                <img src='@((Model.Secratory != null && (!String.IsNullOrEmpty(Model.Secratory.Image.PictureUrl))) ? Model.Secratory.Image.PictureUrl : "/Dolfd/img/111.png")' height="120px" width="100px" />
            </div>
            <div class="member-desc">
                <div class="member-name">
                    @(Model.Secratory != null ? Model.Secratory.Name : "Name of Secretary")                    <!-- <p style="font-weight:  normal;">सम्माननीय प्रधानमन्त्री एवं मन्त्री सञ्चार तथा सूचना प्रविधि ‍</p> -->
                </div>
                <div class="member-postion">Secretary</div>
            </div>
