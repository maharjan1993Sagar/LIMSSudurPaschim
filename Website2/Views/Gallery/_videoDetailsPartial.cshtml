@model GalleryModel
@using LIMS.Website1.Data
@using System.Globalization
@inject IGetLabel _lbl
@{
    var culture = CultureInfo.CurrentCulture.ToString();
    var GoToList = _lbl.GetByName("Go To List", culture);
}


<h2><i class="fa fa-video"></i>@Model.Title</h2>
<div class="col-md-12 text-right">
    <a asp-controller="gallery" asp-action="Index" class="btn">@GoToList</a>
</div>
<div class="col-md-12">

    <iframe width="1000" height="400" src='@(Model.VideoUrl)'
            frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
</div>

