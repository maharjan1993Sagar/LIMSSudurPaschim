@model GalleryModel

<div class="container">
    <div class="row">
        <h2><i class="fa fa-video"></i>@Model.Title</h2>
        <div class="col-md-12 text-right">
            <a asp-controller="gallery" asp-action="Index" class="btn btn-default">Go to list</a>
        </div>
        <div class="col-md-12">

            <iframe width="1000" height="400" src='@(Model.VideoUrl)'
                    frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>

    </div>
</div>