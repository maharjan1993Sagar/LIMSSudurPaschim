@model FarmModel
@{
    //page title
    ViewBag.Title = "Farm";
}
<form method="post" asp-action="create">
    <div class="row">
        <div class="col-md-12">
            <div class="x_panel light form-fit">
                <div class="x_title">
                    <div class="caption">
                        <i class="fa fa-user"></i>
                        @T("Admin.Livestock.Farm.AddNew")
                        <small><i class="fa fa-arrow-circle-left"></i>@Html.ActionLink(T("Admin.Livestock.Farm.BackToList").Text, "List")</small>
                    </div>

                   
                </div>
                <vc:admin-widget widget-zone="blog_details_buttons" additional-data="null" />
                <admin-tabstrip name="farm-edit">
                    <items>
                        <tabstrip-item text="@T("Admin.Livestock.Farm.Info")" tab-index="0">
                            <content>
                                <div>
                                    <partial name="_CreateOrUpdate" model="Model" />
                                  

                                </div>
                                
                            </content>
                        </tabstrip-item>
                    </items>
                </admin-tabstrip>
            </div>
        </div>
    </div>
</form>

