                    Farm Name:@Model.Farm.NameEnglish &nbsp; Mobile no: @Model.Farm.MobileNo &nbsp; District:@Model.Farm.District

                </div>

            </div>
            <div class="x_content form">
                <div class="form-row">
                    <div class="form-body">
                        <input type="hidden" asp-for="FarmId" />
                        <input type="hidden" asp-for="Id" />
                    </div>

                    <div class="col-md-2">

                        <div class="form-group">
                            <admin-label asp-for="Type" />
                            <admin-select asp-for="Type" asp-items="@ViewBag.Type"></admin-select>
                            <span asp-validation-for="Type"></span>

                        </div>
                    </div>

                    <div class="col-md-2 ">

                        <div class="form-group">
                            <admin-label asp-for="NameNepali" />
                            <admin-input asp-for="NameNepali" />
