                            <admin-label asp-for="Email" />
                            <admin-input asp-for="Email" />
                            <span asp-validation-for="Email"></span>

                        </div>
                    </div>

                    <div class="col-md-2">

                        <div class="form-group">
                            <admin-label asp-for="PhoneNo" />
                            <admin-input asp-for="PhoneNo" />
                            <span asp-validation-for="PhoneNo"></span>

                        </div>
                    </div>
                    <div class="col-md-2 farmeronly">

                        <div class="form-group">
                            <admin-label asp-for="EducationQualification" />
                            <admin-select asp-for="EducationQualification" asp-items="ViewBag.Education" />
                            <span asp-validation-for="EducationQualification"></span>

                        </div>
                    </div>
                    <div class="col-md-2 farmeronly">

                        <div class="form-group">
                            <admin-label asp-for="ForeignJobExperience" /><br />
                            <input type="radio" class="radio" asp-for="ForeignJobExperience" value="True" />Yes
                            <input type="radio" class="radio" asp-for="ForeignJobExperience" value="False" />No
                            <span asp-validation-for="ForeignJobExperience"></span>

                        </div>
                    </div>

                    <div class="col-md-2">

                        <div class="form-group">
                            <admin-label asp-for="Gender" /><br />


                            <label>Male</label>
                            <input type="radio" asp-for="Gender" value="Male" />
                            <label>Female</label>
                            <input type="radio" asp-for="Gender" value="Male" />
                            <span asp-validation-for="Gender"></span>

                        </div>
                    </div>
                    <div class="clearfix"></div>

