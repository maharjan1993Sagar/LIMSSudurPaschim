@model LIMS.Web.Areas.Admin.Models.Reports.SubsidyReportModel
<div id="Printcontent">
    <div class="text-center">
        @Model.LocalLevel<br />
        @Model.Level<br />
        @Model.Address<br />
        आ.ब. @Model.FiscalYear को सम्पन्न अनुदानहरुको प्रतिवेदन

    </div>
    <table class="table table-bordered">
        <thead>
            @if (Model.Rows.Count() > 0)
            {
                <tr style="text-align:center">
                    <th id="c">@T("Admin.Subsidy.SN")</th>
                    <th>@T("Admin.Subsidy.BudgetId")</th>
                    <th>@T("Admin.Subsidy.MainActivity")</th>
                    <th>@T("Admin.Subsidy.Remarks")</th>

                </tr>
            }
        </thead>
        <tbody>
            @{string a = "";}
            @foreach (var item in Model.Rows)
            {

            <tr>
                <td>@item.SN</td>
                <td>@item.BudgetTitle</td>
                <td>@item.MainActivity</td>
                <td>@item.Remarks</td>
            </tr>

            }


        </tbody>
    </table>
</div>

@if (Model.Rows.Count() > 0)
{
    <button class="btn btn-success" onclick="Export()">@T("Admin.Button.Export")</button>
}
<script src="~/scripts/jquery.table2excel.js"></script>
<script>
    function Export() {
        $("#Printcontent").table2excel({
            filename: "Subsidy.xls"
        });
    }
</script>
