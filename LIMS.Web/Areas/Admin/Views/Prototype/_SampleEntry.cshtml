

@model LIMS.Web.Areas.Admin.Models.Services.SampleModel
@inject AdminAreaSettings adminAreaSettings
    <div class="row">
<div class="col-md-12">
    <div class="x_panel light form-fit">
        <div class="x_title">
            @*<div class="caption">
                Farm Name: krishna farm &nbsp; Mobile no: 9087656778

            </div>*@
        </div>
        <div class="x_content form" id="services">
            <form method="post" id="OwnerRegistration">
                <div class="row" >
                    <div class="col-md-12">
                        <div class="x_panel light form-fit">

                            <vc:admin-widget widget-zone="blog_details_buttons" additional-data="null" />
                           
                            <div class="x_content form">
                                <div class="form-row">


                                    <div class="col-md-2">

                                        <div class="form-group">
                                            <admin-label asp-for="SampleBoxNo" />
                                            <admin-input asp-for="SampleBoxNo" />
                                            <span asp-validation-for="SampleBoxNo"></span>

                                        </div>
                                    </div>


                                    <div class="col-md-2">

                                        <div class="form-group">
                                            <admin-label asp-for="SampleType" />
                                            <admin-select asp-for="SampleType" asp-items="ViewBag.SampleType" />
                                            <span asp-validation-for="SampleType"></span>

                                        </div>
                                    </div>
                                    <div class="col-md-2">

                                        <div class="form-group">
                                            <admin-label asp-for="LabrotoryName" />
                                            <admin-select asp-for="LabrotoryName" asp-items="ViewBag.LabName" />
                                            <span asp-validation-for="LabrotoryName"></span>

                                        </div>
                                    </div>




                                    <div class="col-md-2">
                                        <div class="form-group No">
                                            <admin-label asp-for="TestingCharge" />
                                            <input asp-for="TestingCharge" type="number" min="1" class="form-control form-rounded" />
                                            <span asp-validation-for="TestingCharge"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group ">
                                            <admin-label asp-for="ReceiptNo" />
                                            <input asp-for="ReceiptNo" class="form-control form-rounded" />
                                            <span asp-validation-for="ReceiptNo"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <admin-label asp-for="Result" />
                                            <admin-input asp-for="Result" />
                                            <span asp-validation-for="Result"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="btn-group btn-group-devided">
                                            <button class="btn btn-success" type="submit" name="save" style="font-size:13px;"><i class="fa fa-check"></i> @T("Admin.Common.Save") </button>
                                            <button class="btn btn-success" type="submit" name="save-continue" style="font-size:13px;"><i class="fa fa-check-circle"></i> @T("Admin.Common.SaveContinue") </button>
                                        </div>
                                    </div>
                                </div>
                                </div>
           
                            </div>
                        </div>
                    </div>
                            
                            </form>
        </div>
    </div>
</div>
</div>
